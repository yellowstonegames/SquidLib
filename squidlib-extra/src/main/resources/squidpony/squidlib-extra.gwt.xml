<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
        "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<!-- To compile to Gwt, you need to reference this file in your master .gwt.xml
     file (usually called GdxDefinition.gwt.xml) as follows:
    <inherits name="regexodus.regexodus"/>
    <inherits name="squidpony.squidlib-util"/>
    <inherits name='squidpony.squidlib-extra' />
  -->
<module>
    <source path=""/>
    <inherits name="squidpony.squidlib-util" />
    <inherits name="regexodus.regexodus"/>
    <extend-configuration-property name="gdx.reflect.include" value="java.util.SortedSet" />
    <extend-configuration-property name="gdx.reflect.include" value="java.util.EnumMap" />
    <extend-configuration-property name="gdx.reflect.include" value="java.util.HashMap" />
    <extend-configuration-property name="gdx.reflect.include" value="java.util.HashSet" />
    <extend-configuration-property name="gdx.reflect.include" value="regexodus.Pattern" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.StringStringMap" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.FakeLanguageGen" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.IntVLA" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.Coord" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.FastNoise" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.RNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.StatefulRNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.GWTRNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.LinnormRNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.DiverRNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.SilkRNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.MiniMover64RNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.GreasedRegion" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.OrderedMap" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.EnumOrderedMap" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.OrderedSet" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.EnumOrderedSet" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.UnorderedSet" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.Arrangement" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.K2" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.K2V1" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath.IntDoubleOrderedMap" />

</module>
